{
    "cSpell.words": [
        "Hasher",
        "nilshenrich"
    ],
    "dart.lineLength": 150,
    "[dart]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.detectIndentation": false,
        "editor.suggest.insertMode": "replace",
        "editor.defaultFormatter": "Dart-Code.dart-code",
        "editor.inlayHints.enabled": "offUnlessPressed",
        "editor.rulers": [
            150
        ]
    },
    "cmake.configureOnOpen": false
}
